<h1><%= "#{@user.firstname} #{@user.lastname}" %></h1>
<h3>Member since: <%= formatted_date(@user.created_at) %></h3>
<h4><%= @user.firstname %> has <%= @user.reviews.length %> reviews</h4>

<%= link_to "Edit", edit_admin_user_path %> | <%= link_to "Delete", admin_user_path(@user), :method => :delete, :"data-confirm" => "You sure?" %>
